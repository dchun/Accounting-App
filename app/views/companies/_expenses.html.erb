<fieldset>
  <%= f.text_area :description, placeholder: "Expense Description" %>
  <%= f.text_field :amount, placeholder: "Expense Amount" %>
  <%= f.text_field :date, placeholder: "Expense Date", class: "date" %>
  <%= f.select(:type, ['Software', 'Infrastructure', 'Custom'], :prompt => "Select Type" ) %>
  <%= f.hidden_field :_destroy %>
  <%= link_to '[remove]', '#', class: 'remove_fields' %>
</fieldset>